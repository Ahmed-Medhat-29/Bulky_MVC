      "destination": "wwwroot/lib/bootswatch/",
      "files": [
        "sandstone/bootstrap.css",
        "sandstone/bootstrap.min.css",
        "sandstone/bootstrap.min.css.map",
        "sandstone/bootstrap.rtl.css",
        "sandstone/bootstrap.rtl.min.css",
        "sandstone/bootstrap.rtl.min.css.map"
      ]
    }
  ]
