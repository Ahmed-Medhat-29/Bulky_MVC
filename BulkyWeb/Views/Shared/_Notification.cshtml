@if (TempData.TryGetValue("success", out object successResult))
{
	<script src="~/lib/toastr.js/toastr.min.js"></script>
	<script type="text/javascript">
		toastr.success('@successResult');
	</script>
}

@if (TempData.TryGetValue("error", out object errorResult))
{
	<script src="~/lib/toastr.js/toastr.min.js"></script>
	<script type="text/javascript">
		toastr.error('@errorResult');
	</script>
}